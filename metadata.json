{
  "name": "opuscodium-ssh",
  "version": "2.1.1-rc0",
  "author": "zleslie",
  "summary": "Puppet ssh management module",
  "license": "Apache-2.0",
  "source": "git://github.com/opus-codium/puppet-ssh.git",
  "project_page": "https://github.com/opus-codium/puppet-ssh",
  "issues_url": "https://github.com/opus-codium/puppet-ssh/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD"
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "12",
        "13"
      ]
    },
    {
      "operatingsystem": "Darwin"
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ],
  "description": "A Puppet module to configure OpenSSH servers",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/sshkeys_core",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0 < 10.0.0"
    }
  ]
}
